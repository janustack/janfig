{
	"extends": "./base.json",
	"display": "Next.js Framework",
	"compilerOptions": {
		"lib": ["DOM", "DOM.Iterable", "ESNext"],
		"jsx": "preserve",
		"incremental": true,
		"plugins": [
			{
				"name": "next"
			}
		],
		"paths": {
			"@/*": ["./src/*"]
		}
	},
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
	"exclude": ["node_modules"]
}
